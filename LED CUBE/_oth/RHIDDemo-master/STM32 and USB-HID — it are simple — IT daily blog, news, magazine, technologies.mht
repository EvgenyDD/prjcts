<div class=3D"spoiler"><b class=3D"spoiler_title">function of HID_Status_In=
</b><div class=3D"spoiler_text"><pre><code class=3D"cpp">/*****************=
**************************************************************
* Function Name  : HID_Status_In.
* Description    : HID status IN routine.
* Input          : None.
* Output         : None.
* Return         : None.
***************************************************************************=
****/
void HID_Status_In(void)
{
  BitAction Led_State;

  if (Report_Buf[1] =3D=3D 0)
  {
    Led_State =3D Bit_RESET;
  }
  else
  {
    Led_State =3D Bit_SET;
  }

  switch (Report_Buf[0])
  {
    case 1: /* Led 1 */
     if (Led_State !=3D Bit_RESET)
     {
       GPIO_SetBits(LED_PORT,LED1_PIN);
     }
     else
     {
       GPIO_ResetBits(LED_PORT,LED1_PIN);
     }
     break;
    case 2: /* Led 2 */
     if (Led_State !=3D Bit_RESET)
     {
       GPIO_SetBits(LED_PORT,LED2_PIN);
     }
     else
     {
       GPIO_ResetBits(LED_PORT,LED2_PIN);
     }
      break;
    case 3: /* Led 1&amp;2 */
       Buffer[4]=3DReport_Buf[1];
     break;
  }
}
